@model List<Product>

@{
    ViewBag.Title = "Bring";
}

<table class="table table-hover">
    <thead>
        <tr>
            <th>ID</th>
            <th>İsim</th>
            <th>Fiyat</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Product item in Model)
        {
            <tr>
                <td>@item.ID</td>
                <td>@item.ProductName</td>
                <td>@item.UnitPrice</td>

            </tr>
        }
    </tbody>
</table>

